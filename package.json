    "ava": "^2.2.0",
    "babel-compile": "^2.0.0",
    "babel-plugin-source-map-support": "^2.0.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-ava": "^7.1.0",
    "eslint-plugin-import": "^2.17.2",
