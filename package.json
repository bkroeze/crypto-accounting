    "pino-pretty": "^2.5.0",
    "source-map-support": "^0.5.6"
  },
  "scripts": {
    "compile": "babel src --out-dir=lib",
    "prepublish": "yarn compile",
    "test": "ava",
    "watch": "ava -v --watch",
    "lint": "eslint src/**"
  },
  "ava": {
    "files": [
      "test/**/*test.js"
    ],
    "sources": [
      "src/**/*.js",
      "test/**/*.yaml"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "compileEnhancements": false,
    "require": [
      "source-map-support",
      "@babel/register",
      "@babel/plugin-transform-regenerator",
      "@babel/polyfill"
    ],
    "sourceMaps": true
  }
}
