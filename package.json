  "files": [
    "lib"
  ],
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "bignumber.js": "^7.2.1",
    "csv-parse": "^4.6.5",
    "csvtojson": "^2.0.8",
    "dotenv": "^6.0.0",
    "folktale": "^2.3.0",
    "graceful-fs": "^4.1.11",
    "js-logger": "^1.4.1",
