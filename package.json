{
  "name": "crypto-accounting",
  "version": "0.0.3",
  "description": "Multi-currency/commodity accounting using human-readable text files",
  "main": "src/cli.js",
  "repository": "https://github.com/bkroeze/crypto-accounting.git",
  "author": "Bruce Kroeze <coindev@mytx.io>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "bignumber.js": "^7.2.1",
    "csv-parse": "^2.5.0",
    "csvtojson": "^2.0.8",
    "dotenv": "^6.0.0",
    "folktale": "^2.3.0",
    "graceful-fs": "^4.1.11",
    "js-logger": "^1.4.1",
    "js-yaml": "^3.12.0",
    "moment": "^2.22.2",
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.9.0",
    "sha.js": "^2.4.11",
    "sorted-array": "^2.0.2",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-compile": "^2.0.0",
    "babel-core": "^6.26.3",
    "babel-plugin-source-map-support": "^2.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-ava": "^4.5.1",
    "eslint-plugin-import": "^2.13.0",
    "source-map-support": "^0.5.6"
  },
  "scripts": {
    "compile": "babel-compile -p stage-3 src:lib",
    "prepublish": "yarn compile",
    "test": "ava",
    "watch": "ava -v --watch",
    "eslint": "eslint"
  },
  "ava": {
    "files": [
      "test/**/*test.js"
    ],
    "sources": [
      "src/**/*.js",
      "test/**/*.yaml"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "compileEnhancements": false,
    "require": [
      "babel-register",
      "source-map-support"
    ],
    "sourceMaps": true
  }
}
