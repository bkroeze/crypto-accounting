{
  "presets": [
    "env",
    "stage-3"
  ],
  "env": {
    "targets" : {
      "node": "current"
    },
    "development": {
      "sourceMaps": "inline",
      "plugins": ["source-map-support"]
    }
  },
  "plugins": [
    "transform-es2015-destructuring",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]]
}
